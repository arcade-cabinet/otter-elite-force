{
  "name": "otter-elite-force",
  "private": true,
  "version": "8.0.0",
  "type": "module",
  "packageManager": "pnpm@10.26.2",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:analyze": "tsc && vite build --mode analyze",
    "bundle-size": "node scripts/extract-bundle-size.js",
    "preview": "vite preview",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format --write .",
    "typecheck": "tsc --noEmit",
    "test": "NODE_OPTIONS='--max-old-space-size=8192' vitest",
    "test:unit": "vitest run --config vitest.config.ts --reporter=verbose",
    "test:unit:watch": "vitest --config vitest.config.ts",
    "test:unit:ui": "vitest --config vitest.config.ts --ui",
    "test:coverage": "NODE_OPTIONS='--max-old-space-size=8192' vitest run --config vitest.config.ts --coverage",
    "test:e2e": "playwright test",
    "test:e2e:mcp": "PLAYWRIGHT_MCP=true playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:visual": "PLAYWRIGHT_MCP=true playwright test e2e/visual-regression.spec.ts",
    "test:e2e:update-snapshots": "PLAYWRIGHT_MCP=true playwright test --update-snapshots",
    "test:all": "pnpm test:unit && pnpm test:e2e",
    "playwright:install": "playwright install --with-deps chromium",
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.4.4",
    "@playwright/test": "^1.58.2",
    "@react-three/test-renderer": "^9.1.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.5.3",
    "@types/node": "^25.3.0",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.2.3",
    "@types/three": "^0.182.0",
    "@vitejs/plugin-react": "^5.1.4",
    "@vitest/coverage-v8": "^4.0.18",
    "@vitest/ui": "^4.0.18",
    "happy-dom": "^20.7.0",
    "jsdom": "^28.1.0",
    "rollup-plugin-visualizer": "^7.0.0",
    "typescript": "~5.9.3",
    "vite": "^7.3.1",
    "vitest": "^4.0.18"
  },
  "dependencies": {
    "@babylonjs/core": "^8.52.0",
    "@babylonjs/havok": "^1.3.11",
    "@babylonjs/loaders": "^8.52.0",
    "@babylonjs/materials": "^8.52.0",
    "@babylonjs/post-processes": "^8.52.0",
    "@babylonjs/react-native": "^1.9.0",
    "@babylonjs/react-native-iosandroid-0-71": "^1.9.0",
    "@miniplex/react": "2.0.0-next.10",
    "@react-hook/window-size": "^3.1.1",
    "@react-native-async-storage/async-storage": "^3.0.1",
    "@react-native/metro-config": "^0.84.0",
    "@strata-game-library/audio-synth": "1.0.2",
    "@strata-game-library/core": "1.4.11",
    "expo": "~52.0.49",
    "expo-asset": "~11.0.5",
    "expo-constants": "~17.0.8",
    "expo-gl": "~15.0.5",
    "expo-status-bar": "~2.0.1",
    "gsap": "^3.14.2",
    "metro": "^0.84.0",
    "metro-config": "^0.84.0",
    "miniplex": "^2.0.0",
    "nipplejs": "^0.10.2",
    "react": "19.2.3",
    "react-device-detect": "^2.2.3",
    "react-dom": "19.2.3",
    "react-native": "0.76.5",
    "react-native-web": "^0.21.2",
    "reactylon": "^3.5.4",
    "recast-detour": "^1.6.4",
    "tone": "^15.1.22",
    "yuka": "^0.7.8",
    "zustand": "^5.0.9"
  }
}
